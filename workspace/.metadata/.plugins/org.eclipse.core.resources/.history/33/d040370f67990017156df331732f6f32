package Logs;

import java.time.LocalDateTime;

import Providers.DateProvider;

public class RefuelLog {
	LocalDateTime dateTime;
	int tankId;
	float volume;
	float stolenVolume;
	int refuelingSpeed = DateProvider.minuteIterator;
	
	public RefuelLog(LocalDateTime dateTime, int tankId, float volume, float stolenVolume) {
		this.dateTime = dateTime;
		this.tankId = tankId;
		this.volume = volume;
		this.stolenVolume = stolenVolume;
	}
}
