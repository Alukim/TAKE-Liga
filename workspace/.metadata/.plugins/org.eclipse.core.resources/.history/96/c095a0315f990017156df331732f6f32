package Logs;

import java.time.LocalDateTime;

public class NozzleMeasuresLog {
	LocalDateTime dateTime;
	int locationId; //empty
	int nozzleId;
	int tankId;
	float literCounter;
	float stolenCounter;
	float totalCounter;
	float stolenVolume;
	int status;
	public NozzleMeasuresLog(LocalDateTime dateTime, int locationId, int nozzleId, int tankId, float literCounter,
			float stolenCounter, float totalCounter, float stolenVolume, int status) {
		this.dateTime = dateTime;
		this.locationId = locationId;
		this.nozzleId = nozzleId;
		this.tankId = tankId;
		this.literCounter = literCounter;
		this.stolenCounter = stolenCounter;
		this.totalCounter = totalCounter;
		this.stolenVolume = stolenVolume;
		this.status = status;
	}
	
}
