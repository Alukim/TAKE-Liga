package Logs;

import java.time.LocalDateTime;

public class TankMeasuresLog {
	LocalDateTime dateTime;
	int locationId; //empty
	int meterId; //empty
	int tankId;
	int tankHigh;
	float currentVolume;
	int temperature;
	int waterHigh = 0;
	int waterVolume = 0;
	public TankMeasuresLog(LocalDateTime dateTime, int tankId, int tankHigh, float currentVolume, int temperature) {
		this.dateTime = dateTime;
		this.tankId = tankId;
		this.tankHigh = tankHigh;
		this.currentVolume = currentVolume;
		this.temperature = temperature;
	}
	
}
