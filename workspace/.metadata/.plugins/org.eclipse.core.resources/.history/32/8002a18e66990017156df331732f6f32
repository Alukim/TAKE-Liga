package Logs;

import java.time.LocalDateTime;

public class NozzleMeasuresLog {
	public LocalDateTime dateTime;
	public int locationId; //empty
	public int nozzleId;
	public int tankId;
	public float literCounter;
	public float stolenCounter;
	public float totalCounter;
	public float stolenVolume;
	public int status;
	public NozzleMeasuresLog(LocalDateTime dateTime, int nozzleId, int tankId, float literCounter,
			float stolenCounter, float totalCounter, float stolenVolume, int status) {
		this.dateTime = dateTime;
		this.nozzleId = nozzleId;
		this.tankId = tankId;
		this.literCounter = literCounter;
		this.stolenCounter = stolenCounter;
		this.totalCounter = totalCounter;
		this.stolenVolume = stolenVolume;
		this.status = status;
	}
	
}
